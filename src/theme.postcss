
:root {
	/* =~= Theme Properties =~= */
	--theme-font-family-base: system-ui;
	--theme-font-family-heading: system-ui;
	--theme-font-color-base: 0 0 0;
	--theme-font-color-dark: 255 255 255;
	--theme-rounded-base: 9999px;
	--theme-rounded-container: 4px; /* ⬅ 8px, edits all borders that must be rounded */
	--theme-border-base: 1px;
	/* =~= Theme On-X Colors =~= */
	--on-primary: 255 255 255;
	--on-secondary: 255 255 255;
	--on-tertiary: 0 0 0;
	--on-success: 0 0 0;
	--on-warning: 0 0 0;
	--on-error: 255 255 255;
	--on-surface: 255 255 255;
	/* =~= Theme Colors  =~= */
	/* primary | #2989FF */
	--color-primary-50: 223 237 255; /* ⬅ #dfedff */
	--color-primary-100: 212 231 255; /* ⬅ #d4e7ff */
	--color-primary-200: 202 226 255; /* ⬅ #cae2ff */
	--color-primary-300: 169 208 255; /* ⬅ #a9d0ff */
	--color-primary-400: 105 172 255; /* ⬅ #69acff */
	--color-primary-500: 41 137 255; /* ⬅ #2989FF */
	--color-primary-600: 37 123 230; /* ⬅ #257be6 */
	--color-primary-700: 31 103 191; /* ⬅ #1f67bf */
	--color-primary-800: 25 82 153; /* ⬅ #195299 */
	--color-primary-900: 20 67 125; /* ⬅ #14437d */
	/* secondary | #FFC457 */
	--color-secondary-50: 255 246 230; /* ⬅ #fff6e6 */
	--color-secondary-100: 255 243 221; /* ⬅ #fff3dd */
	--color-secondary-200: 255 240 213; /* ⬅ #fff0d5 */
	--color-secondary-300: 255 231 188; /* ⬅ #ffe7bc */
	--color-secondary-400: 255 214 137; /* ⬅ #ffd689 */
	--color-secondary-500: 255 196 87; /* ⬅ #FFC457 */
